{
  "name": "awsresources",
  "version": "1.0.0",
  "private":"false",
  "main": "dist/lib/app.js",
  "types": "dist/types/app.d.ts",
  "scripts": {
    "clean": "rm -rf dist && rm -rf cdk.out && rm -rf node_modules",
    "lint": "eslint lib test && prettier --list-different '{lib,test}/*/.ts'",
    "format": "eslint --fix lib test && prettier --write '{lib,test}/*/.ts'",
    "prebuild": "npm run format",
    "build": "tsc",
    "watch": "tsc -w",
    "prepare": "npm run-script build",
    "test": "jest",
    "posttest": "generate-coverage-data --language typescript"
  },
  "jest": {
    "testMatch": [
        "*/*.test.ts"
    ],
    "transform": {
        ".(js|ts)": "ts-jest"
    },
    "transformIgnorePatterns": [
        "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx|json)$",
        "package.json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
        "lib/*/.{ts,js}"
    ],
    "coverageDirectory": "<rootDir>/build/brazil-documentation/coverage",
    "coverageReporters": [
        "cobertura"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "constructs": "^10.0.79",
    "aws-cdk-lib": "^2.14.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "typescript": "~4.3.0",
    "@types/jest": "^28.1.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3"
  }
}
